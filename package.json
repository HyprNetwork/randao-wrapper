{
  "name": "randao-wrapper",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "type": "hardhat typechain",
    "build": "run-s compile type",
    "lint:ts": "eslint 'scripts/*.ts' 'src/*.ts' 'hardhat.config.ts'",
    "lint:tsfix": "eslint 'scripts/**/*.ts 'hardhat.config.ts'' --fix",
    "lint:sol": "solhint 'contracts/*.sol', 'contracts/**/*.sol'",
    "lint:solfix": "solhint 'contracts/*.sol' 'contracts/**/*.sol' --fix",
    "fmt:ts": "prettier --write 'scripts/*.ts' 'src/*.ts' 'hardhat.config.ts'",
    "fmt:sol": "prettier --write 'contracts/*.sol' 'contracts/**/*.sol'",
    "fmt": "run-s fmt:ts fmt:sol",
    "lint": "run-s lint:ts lint:sol",
    "lint:fix": "run-s lint:tsfix lint:solfix",
    "clean": "rimraf build",
    "hardhat:test": "hardhat --network hypr-test",
    "hardhat:dev": "hardhat --network local",
    "hardhat:gsc": "hardhat --network gsc",
    "hardhat:hyprtest": "hardhat --network hypr-test",
    "dep:fac:dev": "yarn hardhat:dev run scripts/deploy_factory.ts",
    "dep:fac:test": "yarn hardhat:test run scripts/deploy_factory.ts",
    "dep:fac:gsc": "yarn hardhat:gsc run scripts/deploy_factory.ts",
    "dep:fac:hyprtest": "yarn hardhat:hyprtest run scripts/deploy_factory.ts",
    "dep:mock:dev": "yarn hardhat:dev run scripts/deploy_mock.ts",
    "dep:mock:test": "yarn hardhat:test run scripts/deploy_mock.ts",
    "dep:mock:gsc": "yarn hardhat:gsc run scripts/deploy_mock.ts",
    "dep:mock:hyprtest": "yarn hardhat:hyprtest run scripts/deploy_mock.ts",
    "dep:wrap:dev": "yarn hardhat:dev run scripts/deploy.ts",
    "dep:wrap:test": "yarn hardhat:test run scripts/deploy.ts",
    "dep:wrap:gsc": "yarn hardhat:gsc run scripts/deploy.ts",
    "dep:wrap:hyprtest": "yarn hardhat:hyprtest run scripts/deploy.ts",
    "dep:dev": "run-s dep:fac:dev dep:wrap:dev dep:mock:dev",
    "dep:test": "run-s dep:fac:test dep:wrap:test dep:mock:test",
    "dep:gsc": "run-s dep:fac:gsc dep:wrap:gsc dep:mock:gsc",
    "dep:hyprtest": "run-s dep:fac:hyprtest dep:wrap:hyprtest dep:mock:hyprtest",
    "start": "node -r ts-node/register -r tsconfig-paths/register -r dotenv/config src/main.ts",
    "test1": "node -r ts-node/register -r tsconfig-paths/register -r dotenv/config src/test1.ts",
    "test2": "node -r ts-node/register -r tsconfig-paths/register -r dotenv/config src/test2.ts",
    "test3": "node -r ts-node/register -r tsconfig-paths/register -r dotenv/config src/test3.ts",
    "test4": "node -r ts-node/register -r tsconfig-paths/register -r dotenv/config src/test4.ts",
    "charge": "node -r ts-node/register -r tsconfig-paths/register -r dotenv/config src/charge.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@chainlink/contracts": "^0.8.0",
    "@confluxfans/contracts": "^0.3.3",
    "@types/debug": "^4.1.10",
    "@types/yargs": "^17.0.29",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "sleep-promise": "^9.1.0",
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-ganache": "^2.0.1",
    "@openzeppelin/hardhat-upgrades": "^2.3.1",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=16.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "chai": "^4.2.0",
    "eslint": "^8.51.0",
    "ethers": "^6.7.1",
    "hardhat": "^2.18.0",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.11.42",
    "hardhat-deploy-ethers": "^0.4.1",
    "hardhat-diamond-abi": "^3.0.1",
    "hardhat-gas-reporter": "^1.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-solidity": "^1.1.3",
    "rimraf": "^5.0.5",
    "solhint": "^3.6.2",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.0",
    "solidity-parse": "^0.0.2",
    "ts-node": ">=8.0.0",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.1",
    "typescript": ">=4.5.0"
  },
  "repository": "https://github.com/HyprNetwork/randao-wrapper.git",
  "author": "root <root@gmail.com>"
}